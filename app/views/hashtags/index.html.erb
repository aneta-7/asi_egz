<ul class="nav navbar-nav pull-right">
    <li><div class = "przycisk"><%= link_to '', links_path, class: "btn btn-default btn-outline fa fa-reply ", title: "Wróć" %></div></li>
</ul>
<h1 class='text-center'>Hasztagi</h1>
  <ul class="nav navbar-nav pull-right">
    <div class= "btn-group">
      <% if @sn %>
        <%= link_to '', { :sortName => "asc" }, class: "btn btn-default btn-outline rozmiar3 glyphicon glyphicon-sort-by-alphabet", title: "Sortuj wg nazwy - malejąco" %>
      <% else %>
        <%= link_to '', { :sortName => "desc" }, class: "btn btn-default btn-outline rozmiar3 glyphicon glyphicon-sort-by-alphabet-alt", title: "Sortuj wg nazwy - roznąco" %>
      <% end %>
      <% if @ss %>
        <%= link_to '', { :sortScore => "asc" }, class: "btn btn-default btn-outline rozmiar3 glyphicon glyphicon-sort-by-attributes", title: "Sortuj wg ilości wystąpień - malejąco" %>
      <% else %>
        <%= link_to '', { :sortScore => "desc" }, class: "btn btn-default btn-outline rozmiar3 glyphicon glyphicon-sort-by-attributes-alt", title: "Sortuj wg ilości wystąpień - roznąco" %>
      <% end %>
    </div class>
  </ul>
<ul class='hasztagi'>
<% @hashtags.each do |hashtag| %>
  <li><%= link_to hashtag.name, hashtag_path(hashtag.name) %></li>
<% end -%>
</ul>
